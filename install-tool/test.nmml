<?xml version="1.0" encoding="utf-8"?>
<project>

  <app
     file="MyApplication"
     title="My Application"
     package="com.package.myapp"
     version="1.0"
     company="nme" 
	  description="NME Application"
  />

  <icons>
     <app svg="icon.svg"/>
     <app image="icon32x32.png"/>
     <app image="icon64x64.png"/>
     <titlebar image="icon32x32.png"/>
     <banner svg="icon.svg" window="zoom"/>
  </icons>

  <window
      width="600"
		height="400"
		orientation="landscape"
		fps="60"
		background="ffffff"
      resizeable="true"
		hardware="true"
	/>

  
  <build>
    <main name="Main" />
    <classpath name="src" />
    <haxelib name="nme" />
  </build>

  <assets>
     <image name="image.png" dir="images"/>
  </assets>

  <targets>
     <target name="swf9"/>
     <target name="android"/>
     <target name="win32" runtme="neko"/>
  </targets>

  <ndlls>
    <ndll name="std"/>
    <ndll name="regexp"/>
    <ndll name="regexp"/>
    <ndll name="nme" haxelib="nme"/>
  </ndlls>

  <certificates>
     <certificate file="signature.cer" name="mysign" password="ajkhsakjhf" />
  </certificates>

</project>
